{
  "name": "fuzzy-testing",
  "version": "0.0.11",
  "description": "Javascript fuzz testing tools",
  "main": "build/fuzzy.cjs.js",
  "jsnext:main": "build/fuzzy.es.js",
  "files": [
    "build",
    "README.md"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "clean": "rimraf build",
    "build": "npm run clean && npm run build:cjs && npm run build:es",
    "build:cjs": "rollup -c -o build/fuzzy.cjs.js -f cjs",
    "build:es": "rollup -c -o build/fuzzy.es.js -f es",
    "test": "jest",
    "lint": "eslint ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dkaoster/Fuzzy.git"
  },
  "keywords": [
    "fuzz",
    "testing",
    "security",
    "node",
    "js"
  ],
  "author": "Daniel Kao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dkaoster/Fuzzy/issues"
  },
  "homepage": "https://github.com/dkaoster/Fuzzy#readme",
  "devDependencies": {
    "eslint": "^7.20.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "jest": "^26.6.3",
    "react": "^17.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.39.1",
    "rollup-jest": "^1.1.1"
  },
  "dependencies": {
    "prop-types": "^15.7.2"
  },
  "jest": {
    "preset": "rollup-jest"
  }
}
